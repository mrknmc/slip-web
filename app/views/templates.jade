script(type='text/template', id='upload-template')
  td <%- user.name %>
  td <%- user.email %>
  td <%- deviceId %>
  td <%- created %>
  td
    | <a href="/api/measurements/<%= _id %>" target="_blank"><%- _id %></a>

script(type='text/template', id='chart-template')
  div.panel.panel-default
    div.panel-heading.container-fluid
      .row
        h3.panel-title.col-xs-2.col-sm-4 <%- _id %>
        form.form-horizontal.datefilter.col-xs-10.col-sm-8.row
          .form-group.pull-right.col-xs-5.row
            label.control-label.col-xs-5 End Date
            div.col-xs-7
              input.form-control.datepicker.end(type='text', placeholder='End Date')
          .form-group.pull-right.col-xs-5.row
            label.control-label.col-xs-5 Start Date
            div.col-xs-7
              input.form-control.datepicker.start(type='text', placeholder='Start Date')
    div.panel-body
